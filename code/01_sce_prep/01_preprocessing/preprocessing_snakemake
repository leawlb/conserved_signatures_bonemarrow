snakemake --cluster "bsub -n2 -q long -R rusage[mem=10GB]" -p -j66 -c10 -s preprocessing_snakefile.py --configfile ../../config.yaml --latency-wait 30 -n 

snakemake --cluster "bsub -n2 -q long -R rusage[mem=10GB]" -p -j66 -c10 -s preprocessing_snakefile.py --configfile ../../config.yaml --latency-wait 30 -R remove_droplets -n 

snakemake --cluster "bsub -n2 -q long -R rusage[mem=100GB]" -p -j66 -c20 -s preprocessing_snakefile.py --configfile  ../../config.yaml --config run_preprocessing_summary=True -R make_summary_report -n

