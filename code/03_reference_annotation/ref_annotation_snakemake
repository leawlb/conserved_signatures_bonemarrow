The longest sample takes >28h

snakemake --cluster "bsub -n2 -q verylong -R rusage[mem=100GB]" -p -j66 -c4 -s reference_annotation_snakefile.py --configfile ../config/config.yaml -n

snakemake --cluster "bsub -n2 -q verylong -R rusage[mem=20GB]" -p -j66 -c4 -s reference_annotation_snakefile.py --configfile ../config/config.yaml -n

snakemake --cluster "bsub -n2 -q verylong -R rusage[mem=100GB]" -p -j66 -c10 -s reference_annotation_snakefile.py --configfile ../config/config.yaml -R cell_type_annotation -n 

snakemake -s reference_annotation_snakefile.py --configfile ../config/config.yaml --cores 4 -n

snakemake --cluster "bsub -q long -R rusage[mem=50GB]" -p -j66 -c4 -s reference_annotation_snakefile.py --configfile ../config/config.yaml -R make_ref_sample_reports -n

snakemake --cluster "bsub -q verylong -R rusage[mem=50GB]" -p -j66 -c4 -s reference_annotation_snakefile.py --configfile ../config/config.yaml -R make_ref_summary_report -n

snakemake --cluster "bsub -q verylong -R rusage[mem=50GB]" -p -j66 -s reference_annotation_snakefile.py --configfile ../config/config.yaml --config run_ref_annotation_summary=True -R make_ref_summary_report -n