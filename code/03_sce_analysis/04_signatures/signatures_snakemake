snakemake --cluster "bsub -q medium -n20 -R rusage[mem=70GB]" -p -j10 -c20 -s signatures_snakefile.py --latency-wait 20 --configfile ../../config.yaml --use-conda  -n

# for permutation 
snakemake --cluster "bsub -q medium -n20 -R rusage[mem=200GB]" -p -j10 -c20 -s signatures_snakefile.py --latency-wait 20 --configfile ../../config.yaml --use-conda --config recluster_other=True -n

# for testing
snakemake --cluster "bsub -q medium -n20 -R rusage[mem=70GB]" -p -j10 -c20 -s signatures_snakefile.py --latency-wait 20 --configfile ../../config.yaml --use-conda --config recluster_other=True -n