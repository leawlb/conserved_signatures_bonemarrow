
snakemake --cluster "bsub -q verylong -R rusage[mem=100GB]" -p -j5 -c4 -s batch_correction_snakefile.py --configfile ../config/config.yaml -n

snakemake --cluster "bsub -n20 -q verylong -R rusage[mem=200GB]" -p -j8 -c40 -s batch_correction_snakefile.py --configfile ../config/config.yaml -R run_mnncorrect -n

snakemake --cluster "bsub -n4 -q verylong -R rusage[mem=100GB]" -p -j8 -c10 -s batch_correction_snakefile.py --configfile ../config/config.yaml -R make_reports -n

snakemake --cores 2 -s batch_correction_snakefile.py --configfile ../config/config.yaml  -n

snakemake --cores 2 -s batch_correction_snakefile.py --configfile ../config/config.yaml --config run_batch_correction_summary=True -n
